<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sublayout_practice_count_back">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:paddingBottom="180dp"
        android:paddingLeft="5dp"
        android:orientation="vertical"
        style="@style/StatisticPracticeTime">
        <LinearLayout
            android:id="@+id/_text_group_1"
            android:layout_width="wrap_content"
            android:layout_height="50sp"
            android:padding="5sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="left"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/_label_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="你一共练习了"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>

            <TextView
                android:id="@+id/sublayout_practice_count_score"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="183"
                android:paddingLeft="15sp"
                android:paddingRight="15sp"
                android:textSize="35sp"
                android:fontFamily="@font/artifakt"/>

            <TextView
                android:id="@+id/_label_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="首曲子"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/_text_group_2"
            android:layout_width="wrap_content"
            android:layout_height="50sp"
            android:padding="5sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="left"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/_label_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="所有曲子共计练习了"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>

            <TextView
                android:id="@+id/sublayout_practice_count_total"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="2273"
                android:paddingLeft="15sp"
                android:paddingRight="15sp"
                android:textSize="35sp"
                android:fontFamily="@font/artifakt"/>

            <TextView
                android:id="@+id/_label_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="遍"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/_text_group_3"
            android:layout_width="wrap_content"
            android:layout_height="50sp"
            android:padding="5sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="left"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/_label_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="这首"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
            <TextView
                android:id="@+id/sublayout_practice_count_max_score_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="《秋日私语》"
                android:paddingLeft="6sp"
                android:paddingRight="6sp"
                android:textSize="28sp"
                android:fontFamily="@font/zongyi"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/_text_group_4"
            android:layout_width="wrap_content"
            android:layout_height="50sp"
            android:padding="5sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="left"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/_label_12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="你反反复复练习了"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
            <TextView
                android:id="@+id/sublayout_practice_count_max_score_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="39"
                android:paddingLeft="10sp"
                android:paddingRight="10sp"
                android:textSize="35sp"
                android:fontFamily="@font/artifakt"/>
            <TextView
                android:id="@+id/_label_6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="次"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/_text_group_5"
            android:layout_width="wrap_content"
            android:layout_height="50sp"
            android:padding="5sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="left"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/_label_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="相信你一定印象深刻吧 ~"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
        </LinearLayout>

    </LinearLayout>
</RelativeLayout>