<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sublayout_practice_time_back">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:paddingBottom="180dp"
        android:paddingRight="5dp"
        android:orientation="vertical"
        style="@style/StatisticPracticeTime">
        <LinearLayout
            android:id="@+id/_text_group_1"
            android:layout_width="wrap_content"
            android:layout_height="50sp"
            android:padding="5sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="right"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/_label_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="从你使用"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
            <TextView
                android:id="@+id/_label_7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="Painist"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:textSize="24sp"
                android:textStyle="bold"
                android:fontFamily="@font/artifakt"/>
            <TextView
                android:id="@+id/_label_8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="以来 ..."
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/_text_group_2"
            android:layout_width="wrap_content"
            android:layout_height="50sp"
            android:padding="5sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="right"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/_label_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="你一共练习了"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>

            <TextView
                android:id="@+id/sublayout_practice_time_total"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="233"
                android:paddingLeft="15sp"
                android:paddingRight="15sp"
                android:textSize="35sp"
                android:fontFamily="@font/artifakt"/>

            <TextView
                android:id="@+id/_label_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="小时"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/_text_group_3"
            android:layout_width="wrap_content"
            android:layout_height="50sp"
            android:padding="5sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="right"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/sublayout_practice_time_max_date_year"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="2020"
                android:paddingLeft="6sp"
                android:paddingRight="6sp"
                android:textSize="28sp"
                android:fontFamily="@font/artifakt"/>
            <TextView
                android:id="@+id/_label_9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="年"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
            <TextView
                android:id="@+id/sublayout_practice_time_max_date_month"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="4"
                android:paddingLeft="6sp"
                android:paddingRight="6sp"
                android:textSize="28sp"
                android:fontFamily="@font/artifakt"/>
            <TextView
                android:id="@+id/_label_10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="月"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
            <TextView
                android:id="@+id/sublayout_practice_time_max_date_day"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="1"
                android:paddingLeft="6sp"
                android:paddingRight="6sp"
                android:textSize="28sp"
                android:fontFamily="@font/artifakt"/>
            <TextView
                android:id="@+id/_label_11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="日"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
            <TextView
                android:id="@+id/_label_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="那一天"
                android:paddingLeft="10sp"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/_text_group_4"
            android:layout_width="wrap_content"
            android:layout_height="50sp"
            android:padding="5sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="right"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/_label_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="你不知疲倦地"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/_text_group_5"
            android:layout_width="wrap_content"
            android:layout_height="50sp"
            android:padding="5sp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_gravity="right"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/_label_12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="练习了"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
            <TextView
                android:id="@+id/sublayout_practice_time_max"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="11.5"
                android:paddingLeft="10sp"
                android:paddingRight="10sp"
                android:textSize="35sp"
                android:fontFamily="@font/artifakt"/>
            <TextView
                android:id="@+id/_label_6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="小时"
                android:textSize="22sp"
                android:fontFamily="@font/zongyi"/>
        </LinearLayout>

    </LinearLayout>
</RelativeLayout>